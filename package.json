{
  "name": "automagic-code",
  "displayName": "Automagic Code",
  "description": "Make your life easier to use Automagic Code with Mustache Logic-less",
  "version": "0.0.1",
  "publisher": "jairzjunior",
  "engines": {
    "vscode": "^1.12.0"
  },
  "categories": [    
    "Other"
  ],
  "activationEvents": [
    "onCommand:automagicCode.generate"
  ],
  "main": "./out/src/extension",
  "icon": "images/icon.png",
  "license": "SEE LICENSE IN LICENSE.md",
  "homepage": "https://github.com/jairzjunior/vscode-automagic-code/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/jairzjunior/vscode-automagic-code.git"
  },
  "bugs": {
    "url": "https://github.com/jairzjunior/vscode-automagic-code/issues"
  },
  "contributes": {
    "commands": [
      {
        "command": "automagicCode.generate",
        "title": "Automagic Code: Generate"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "mocha": "^2.3.3",
    "typescript": "^2.0.3",
    "vscode": "^1.0.0"
  },
  "dependencies": {
    "vscode-set-text": "^1.0.0",
    "mu2": "^0.5"
  }
}
